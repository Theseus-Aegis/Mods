<?xml version="1.0" encoding="utf-8"?>
<Project name="TAC">
    <Package name="ShootingRange">
        <Key ID="STR_TAC_ShootingRange_Range">
            <English>Shooting Range</English>
            <German>Schießstand</German>
        </Key>
        <Key ID="STR_TAC_ShootingRange_ModuleDesc">
            <English>This module allows you to set up a Shooting Range.</English>
            <German>Dieses Modul erlaubt dir einen Schießstand einzurichten.</German>
        </Key>
        <Key ID="STR_TAC_ShootingRange_Name">
            <English>Name</English>
            <German>Name</German>
        </Key>
        <Key ID="STR_TAC_ShootingRange_NameDesc">
            <English>Name of the current range.</English>
            <German>Name des aktuellen Schießstands.</German>
        </Key>
        <Key ID="STR_TAC_ShootingRange_Targets">
            <English>Targets</English>
            <German>Ziele</German>
        </Key>
        <Key ID="STR_TAC_ShootingRange_TargetsDesc">
            <English>List of object names used as targets, separated by commas. Can be synchronized objects.</English>
            <German>Liste der Objektnamen, die als Ziele benutzt werden. Durch Kommas trennen. Die Objekte können synchronisiert sein.</German>
        </Key>
        <Key ID="STR_TAC_ShootingRange_Hits">
            <English>Hits</English>
        </Key>
        <Key ID="STR_TAC_ShootingRange_HitsDesc">
            <English>List of numbers used as hits the target takes before falling, separated by commas.</English>
        </Key>
        <Key ID="STR_TAC_ShootingRange_TargetsInvalid">
            <English>Invalid Targets</English>
        </Key>
        <Key ID="STR_TAC_ShootingRange_TargetsInvalidDesc">
            <English>List of object names used as invalid targets, separated by commas. Must be same amount as targets when used in Trigger mode, fill empty slots with 'nil'.</English>
        </Key>
        <Key ID="STR_TAC_ShootingRange_Controllers">
            <English>Controllers</English>
            <German>Regler</German>
        </Key>
        <Key ID="STR_TAC_ShootingRange_ControllersDesc">
            <English>List of object names used as controllers, separated by commas.</English>
            <German>Liste der Objektnamen, die als Regler genutzt werden. Durch Kommas trennen.</German>
        </Key>
        <Key ID="STR_TAC_ShootingRange_SoundSources">
            <English>Sound Sources</English>
        </Key>
        <Key ID="STR_TAC_ShootingRange_SoundSourcesDesc">
            <English>List of object names used as sound sources (in addition to controllers), separated by commas.</English>
        </Key>
        <Key ID="STR_TAC_ShootingRange_Mode">
            <English>Mode</English>
            <German>Modus</German>
        </Key>
        <Key ID="STR_TAC_ShootingRange_ModeDesc">
            <English>Time and Hit require Duration, Trigger requires Trigger Markers. Default: Timed</English>
            <German>Zeit- und Treffermodus benötigen Zeit. Auslösermodus benötigt Auslöser-Markierungen. Standard: Zeitmodus</German>
        </Key>
        <Key ID="STR_TAC_ShootingRange_Durations">
            <English>Durations</English>
            <German>Laufzeiten</German>
        </Key>
        <Key ID="STR_TAC_ShootingRange_DurationsDesc">
            <English>List of available shooting durations in seconds, separated by commas. Only used in Timed, Hit (Time Limited) and Rampage Modes. Default: 0, 30, 60, 150, 300</English>
        </Key>
        <Key ID="STR_TAC_ShootingRange_DefaultDuration">
            <English>Default Duration</English>
            <German>Standardzeit</German>
        </Key>
        <Key ID="STR_TAC_ShootingRange_DefaultDurationDesc">
            <English>Default shooting duration in seconds. Only used in Timed, Hit (Time Limited) and Rampage Modes. Default: 60</English>
        </Key>
        <Key ID="STR_TAC_ShootingRange_TargetAmounts">
            <English>Target Amounts</English>
            <German>Anzahl der Ziele</German>
        </Key>
        <Key ID="STR_TAC_ShootingRange_TargetAmountsDesc">
            <English>List of available number of target pop-ups, separated by commas. Only used in Hit (Target Limited) Mode. Default: 5, 10, 15, 30, 45</English>
            <German>Liste der möglichen Zahl aufklappbarer Ziele. Durch Kommas trennen. Wird nur im Treffermodus (Ziellimit) genutzt. Standard: 15</German>
        </Key>
        <Key ID="STR_TAC_ShootingRange_DefaultTargetAmount">
            <English>Default Target Amount</English>
            <German>Standard Anzahl an Zielen</German>
        </Key>
        <Key ID="STR_TAC_ShootingRange_DefaultTargetAmountDesc">
            <English>Default amount of target pop-ups. Only used in Hit (Target Limited) Mode. Default: 15</English>
            <German>Standard Anzahl an aufklappbaren Ziele. Wird nur im Treffermodus (Ziellimit) genutzt. Standard: 15</German>
        </Key>
        <Key ID="STR_TAC_ShootingRange_PauseDurations">
            <English>Pause Durations</English>
            <German>Pausenlängen</German>
        </Key>
        <Key ID="STR_TAC_ShootingRange_PauseDurationsDesc">
            <English>List of available pause durations during target pop-ups in seconds, separated by commas. Only used in Timed Mode. Default: 1, 2, 3, 4, 5</English>
            <German>Liste der möglichen Pausen für aufklappende Ziele. Durch Kommas trennen. Wird nur im Zeitmodus genutzt. Standard: 1, 2, 3, 4, 5</German>
        </Key>
        <Key ID="STR_TAC_ShootingRange_DefaultPauseDuration">
            <English>Default Pause Duration</English>
            <German>Standard-Pausenlänge</German>
        </Key>
        <Key ID="STR_TAC_ShootingRange_DefaultPauseDurationDesc">
            <English>Default pause duration during target pop-ups in seconds. Default: 3</English>
            <German>Standard-Pausenlänge während Ziele aufklappen in Sekunden. Standard: 3</German>
        </Key>
        <Key ID="STR_TAC_ShootingRange_CountdownTimes">
            <English>Countdown Times</English>
            <German>Countdown-Zeiten</German>
        </Key>
        <Key ID="STR_TAC_ShootingRange_CountdownTimesDesc">
            <English>List of times it takes from pressing start to first target pop-up. Default: 6, 9, 12, 15</English>
            <German>Liste der möglichen Zeiten die es dauert, bis nach dem Druck auf Start die ersten Ziele aufklappen. Standard: 6, 9, 12, 15</German>
        </Key>
        <Key ID="STR_TAC_ShootingRange_DefaultCountdownTime">
            <English>Default Countdown Time</English>
            <German>Standard Countdown-Zeit</German>
        </Key>
        <Key ID="STR_TAC_ShootingRange_DefaultCountdownTimeDesc">
            <English>Default time it takes from pressing start to first target pop-up. Default: 9</English>
            <German>Standard Countdown-Zeit die benötigt wird, bis nach dem Druck auf Start die ersten Ziele aufklappen. Standard: 9</German>
        </Key>
        <Key ID="STR_TAC_ShootingRange_TriggerMarkers">
            <English>Trigger Markers</English>
            <German>Auslöser-Markierungen</German>
        </Key>
        <Key ID="STR_TAC_ShootingRange_TriggerMarkersDesc">
            <English>List of trigger markers used for controlled target pop-ups, separated by commas. Must be in order with targets. Available in Trigger Mode only.</English>
            <German>Liste der Auslöser-Markierungen die für gezielt aufklappbare Ziele genutzt werden können. Durch Kommas trennen. Muss in der Reihenfolge der Ziele sein. Nur im Auslösermodus verfügbar.</German>
        </Key>
        <Key ID="STR_TAC_ShootingRange_PopOnTriggerExit">
            <English>Pop on Trigger Exit</English>
        </Key>
        <Key ID="STR_TAC_ShootingRange_PopOnTriggerExitDesc">
            <English>Pop targets down when exiting the trigger. Available in Trigger Mode only.</English>
        </Key>
        <Key ID="STR_TAC_ShootingRange_ShowHits">
            <English>Show Hits</English>
        </Key>
        <Key ID="STR_TAC_ShootingRange_ShowHitsDesc">
            <English>Show number of hits out of number of hits necessary on each hit. Available only when more than 1 hit is necessary.</English>
        </Key>
        <Key ID="STR_TAC_ShootingRange_Timed">
            <English>Timed</English>
            <German>Auf Zeit</German>
        </Key>
        <Key ID="STR_TAC_ShootingRange_Hit">
            <English>Hit</English>
            <German>Treffer</German>
        </Key>
        <Key ID="STR_TAC_ShootingRange_HitTimeLimit">
            <English>Hit (Time Limit)</English>
            <German>Treffer (Zeitlimit)</German>
        </Key>
        <Key ID="STR_TAC_ShootingRange_HitTargetLimit">
            <English>Hit (Target Limit)</English>
            <German>Treffer (Ziellimit)</German>
        </Key>
        <Key ID="STR_TAC_ShootingRange_Trigger">
